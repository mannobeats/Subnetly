generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Device {
  id         String   @id @default(cuid())
  name       String
  macAddress String
  ipAddress  String
  category   String
  notes      String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
